<h2>Users list</h2>
<%= link_to "Create new user", new_admin_user_path, class: "badge badge-success" %>
<% i = 0 %>
<% @users.each do |user| %>
	<p> <%= i+= 1 %> : <%= user.name %>
		<% @id = user.id%>
		<%= link_to "Modifier", edit_admin_user_path(user.id) %> <%= link_to 'Supprimer', admin_user_path(user.id), method: :delete ,id: "delete#{@id}"%> <%= link_to 'Show punctualities', admin_user_path(user.id) %> 
	</p>
<% end %>

<%= link_to "Back to home", punctualities_path, class: "badge badge-success" %>